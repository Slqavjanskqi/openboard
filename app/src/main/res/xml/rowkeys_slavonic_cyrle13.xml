<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0448: "ш" CYRILLIC LETTER š -->
    <Key
        latin:additionalMoreKeys="*,ⱎ,щ,ⱋ,ⷲ,ⷳ"
        latin:keyHintLabel="*"
        latin:keySpec="&#x0448;" />
    <!-- U+0445: "х" CYRILLIC LETTER x -->
    <Key
        latin:additionalMoreKeys="&quot;,ⱒ,ⷯ"
        latin:keyHintLabel="&quot;"
        latin:keySpec="&#x0445;" />
    <!-- U+0446: "ц" CYRILLIC LETTER c -->
    <Key
        latin:additionalMoreKeys="&apos;,ⱌ,ҁ,ⷰ"
        latin:keyHintLabel="&apos;"
        latin:keySpec="&#x0446;" />
    <!-- U+0432: "в" CYRILLIC LETTER v -->
    <Key
        latin:additionalMoreKeys=":,ⰲ,ѵ,ѷ,ⱛ,ⷡ"
        latin:keyHintLabel=":"
        latin:keySpec="&#x0432;" />
    <!-- U+0431: "б" CYRILLIC LETTER b -->
    <Key
        latin:additionalMoreKeys=";,ⰱ,ⷠ"
        latin:keyHintLabel=";"
        latin:keySpec="&#x0431;" />
    <!-- U+043D: "н" CYRILLIC LETTER n -->
    <Key
        latin:additionalMoreKeys="!,ⱀ,њ,ⷩ"
        latin:keyHintLabel="!"
        latin:keySpec="&#x043D;" />
    <!-- U+043C: "м" CYRILLIC LETTER m -->
    <Key
        latin:additionalMoreKeys="\\?,ⰿ,ⱞ,ⷨ"
        latin:keyHintLabel="\?"
        latin:keySpec="&#x043C;" />
    <!-- U+0456: "і" CYRILLIC LETTER i -->
    <Key
        latin:additionalMoreKeys="&lt;,ⱜ,ї,ⰹ,ꙶ"
        latin:keyHintLabel="&lt;"
        latin:keySpec="&#x0456;" />
    <!-- U+044C: "ь" CYRILLIC LETTER ʲ -->
    <Key
        latin:additionalMoreKeys="&gt;,ⱐ,ꙺ,ꚝ"
        latin:keyHintLabel="&gt;"
        latin:keySpec="&#x044C;" />
</merge>