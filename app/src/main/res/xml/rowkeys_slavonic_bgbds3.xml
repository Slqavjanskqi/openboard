<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0458: "ј" CYRILLIC LETTER ј -->
    <Key
        latin:additionalMoreKeys="*,ї,ⰹ,ꙶ"
        latin:keyHintLabel="*"
        latin:keySpec="&#x0458;" />
    <!-- U+044C: "ь" CYRILLIC LETTER ʲ -->
    <Key
        latin:additionalMoreKeys="&quot;,ⱐ,ꙺ,ꚝ"
        latin:keyHintLabel="&quot;"
        latin:keySpec="&#x044C;" />
    <!-- U+0456: "і" CYRILLIC LETTER i -->
    <Key
        latin:additionalMoreKeys="&apos;,ⱜ,ї,ⰹ,ꙶ"
        latin:keyHintLabel="&apos;"
        latin:keySpec="&#x0456;" />
    <!-- U+044B: "ы" CYRILLIC LETTER y -->
    <Key
        latin:additionalMoreKeys=":,ꙑ,ъї,ⱏⰹ,ꙹ"
        latin:keyHintLabel=":"
        latin:keySpec="&#x044B;" />
    <!-- U+0444: "ф" CYRILLIC LETTER f -->
    <Key
        latin:additionalMoreKeys=";,ⱇ,ѳ,ⱚ,ⷴ,ꚛ"
        latin:keyHintLabel=";"
        latin:keySpec="&#x0444;" />
    <!-- U+0445: "х" CYRILLIC LETTER x -->
    <Key
        latin:additionalMoreKeys="!,ⱒ,ⷯ"
        latin:keyHintLabel="!"
        latin:keySpec="&#x0445;" />
    <!-- U+043F: "п" CYRILLIC LETTER p -->
    <Key
        latin:additionalMoreKeys="\\?,ⱂ,ⱊ,ⷫ"
        latin:keyHintLabel="\?"
        latin:keySpec="&#x043F;" />
    <!-- U+0440: "р" CYRILLIC LETTER r -->
    <Key
        latin:additionalMoreKeys="&lt;,ⱃ,ⷬ"
        latin:keyHintLabel="&lt;"
        latin:keySpec="&#x0440;" />
    <!-- U+043B: "л" CYRILLIC LETTER l -->
    <Key
        latin:additionalMoreKeys="&gt;,ⰾ,љ,ⷧ"
        latin:keyHintLabel="&gt;"
        latin:keySpec="&#x043B;" />
</merge>