        latin:additionalMoreKeys="#,ꙑ,ъї,ⱏⰹ,ꙹ"
        latin:keyHintLabel="#"
        latin:keySpec="&#x044B;" />
    <!-- U+0432: "в" CYRILLIC LETTER v -->
    <Key
        latin:additionalMoreKeys="$,ⰲ,ѵ,ѷ,ⱛ,ⷡ"
        latin:keyHintLabel="$"
        latin:keySpec="&#x0432;" />
    <!-- U+0430: "а" CYRILLIC LETTER a -->
    <Key
        latin:additionalMoreKeys="%,ⰰ,ꙗ,ⱝ,я,ѣ,ⱑ,ⷶ,ⷺ,ⷼ,ꙓ"
        latin:keyHintLabel="%"
        latin:keySpec="&#x0430;" />
