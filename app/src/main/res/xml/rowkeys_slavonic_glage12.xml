<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0430: "а" CYRILLIC LETTER a -->
    <Key
        latin:additionalMoreKeys="\\@,а,ꙗ,ⱝ,я,ѣ,ⱑ,ⷶ,ⷺ,ⷼ,ꙓ"
        latin:keyHintLabel="\@"
        latin:keySpec="ⰰ" />
    <!-- U+0441: "с" CYRILLIC LETTER s -->
    <Key
        latin:additionalMoreKeys="#,с,ѕ,ⷭ"
        latin:keyHintLabel="#"
        latin:keySpec="ⱄ" />
    <!-- U+0434: "д" CYRILLIC LETTER l -->
    <Key
        latin:additionalMoreKeys="$,д,ђ,ⰼ,ⷣ"
        latin:keyHintLabel="$"
        latin:keySpec="ⰴ" />
    <!-- U+0444: "ф" CYRILLIC LETTER f -->
    <Key
        latin:additionalMoreKeys="%,ф,ѳ,ⱚ,ⷴ,ꚛ"
        latin:keyHintLabel="%"
        latin:keySpec="ⱇ" />
    <!-- U+0436: "ж" CYRILLIC LETTER ǧ -->
    <Key
        latin:additionalMoreKeys="&amp;,ж,џ,ⷤ"
        latin:keyHintLabel="&amp;"
        latin:keySpec="ⰶ" />
    <!-- U+0433: "г" CYRILLIC LETTER h -->
    <Key
        latin:additionalMoreKeys="_,г,ґ,ⰳ,ⷢ"
        latin:keyHintLabel="_"
        latin:keySpec="ⱈ" />
    <!-- U+0458: "ј" CYRILLIC LETTER ј -->
    <Key
        latin:additionalMoreKeys="/,ї,ј,ꙶ"
        latin:keyHintLabel="/"
        latin:keySpec="ⰹ" />
    <!-- U+043A: "к" CYRILLIC LETTER k -->
    <Key
        latin:additionalMoreKeys="-,к,ⷦ"
        latin:keyHintLabel="-"
        latin:keySpec="ⰽ" />
    <!-- U+043B: "л" CYRILLIC LETTER l -->
    <Key
        latin:additionalMoreKeys="+,л,љ,ⷧ"
        latin:keyHintLabel="+"
        latin:keySpec="ⰾ" />
    <!-- U+0447: "ч" CYRILLIC LETTER č -->
    <Key
        latin:additionalMoreKeys="(,ч,ћ,ꙉ,ⷱ,ⷸ"
        latin:keyHintLabel="("
        latin:keySpec="ⱍ" />
    <!-- U+0467: "ѧ" CYRILLIC LETTER æ -->
    <Key
        latin:additionalMoreKeys="),ѧ,ѩ,ⱕ,ⷽ"
        latin:keyHintLabel=")"
        latin:keySpec="ⱔ" />
</merge>